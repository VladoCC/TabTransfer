{
  "name": "Tab Transit",
  "description": "Export and Import your currently openned tabs to/from a file",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "downloads"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
